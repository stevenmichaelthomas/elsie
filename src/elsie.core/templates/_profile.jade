md-dialog
  md-dialog-content(layout="row",layout-wrap)
    md-input-container(flex="100")
      label First name
      input(type="text",ng-model="profile.firstName")
    md-input-container(flex="100")
      label Last name
      input(type="text",ng-model="profile.lastName")
  md-dialog-actions
    md-button.md-primary(ng-click="closeDialog()") Save